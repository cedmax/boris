{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "react/jsx-uses-vars": 2,
    "indent": [2, 2, { "SwitchCase": 1 }],
    "quotes": [2, "single"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "react/jsx-no-undef": 2,
    "react/prop-types": 2,
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "plugins": [
    "react"
  ]
}
