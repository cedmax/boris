{
	"preset": "mdcs",
	"requirePaddingNewlinesInBlocks": null,
	"requireLineFeedAtFileEnd": null,
	"requireSpacesInsideParentheses": { "all": true, "except": [ "(", ")", "{", "}" ] },
	"disallowSpacesInsideParentheses": { "only": [ "(", ")", "{", "}" ] },
	"requireSpaceAfterPrefixUnaryOperators": null,
	"disallowSpaceAfterPrefixUnaryOperators": ["++", "--", "+", "-", "~", "!"],
	"disallowKeywords": null,
	"validateIndentation": 2,
	"disallowVar": true,
	"disallowSpacesInsideTemplateStringPlaceholders": true,
	"requireArrowFunctions": true,
	"requireEnhancedObjectLiterals": true,
	"requireObjectDestructuring": true,
	"requireObjectKeysOnNewLine": true,
	"requireShorthandArrowFunctions": true,
	"requireTemplateStrings": true
}
